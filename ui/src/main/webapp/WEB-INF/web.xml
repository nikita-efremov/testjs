<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <filter>
        <filter-name>SomeFilter</filter-name>
        <filter-class>ru.tsystems.tsproject.rzd.SomeServletFilter</filter-class>
        <init-param>
            <param-name>filterParam1Name</param-name>
            <param-value>filterParam1Value</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>SomeFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <listener>
        <listener-class>ru.tsystems.tsproject.rzd.SomeListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>HelloWorld</servlet-name>
        <display-name>HelloWorld</display-name>
        <description></description>
        <servlet-class>ru.tsystems.tsproject.rzd.HelloWorld</servlet-class>
        <init-param>
            <param-name>servletParam1Name</param-name>
            <param-value>servletParam1Value</param-value>
        </init-param>
    </servlet>
  <servlet-mapping>
  	<servlet-name>HelloWorld</servlet-name>
  	<url-pattern>/HelloWorld</url-pattern>
  </servlet-mapping>
</web-app>
